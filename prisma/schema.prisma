datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id Int @id @default(autoincrement())
    userType UserType
    userAddress String @unique
    matricNumber String @unique
    cgpa Int
    fullName String
}

enum UserType {
    NONE
    STUDENT
    CM
}
